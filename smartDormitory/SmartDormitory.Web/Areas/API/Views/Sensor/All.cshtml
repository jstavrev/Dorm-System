@model AllSensorsViewModel
@{
    ViewData["Title"] = "All";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<br />

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Tag</th>
            <th>Value</th>
            <th>Type</th>
            <th>LastUpdate</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var sensor in Model.sensors)
        {
            <tr>
                <td>@sensor.Tag</td>
                <td>@sensor.Value</td>
                <td>@sensor.MeasureType</td>
                <td>@sensor.TimeStamp</td>
            </tr>
        }
    </tbody>
</table>

